<div class="container p-md-5 pt-4" id="electives">
    <div class="h1-back-red d-flex align-items-center justify-content-center mb-3">
        <h1 class="h1">Факультативы</h1>
    </div>
    <div class="d-flex justify-content-center">
        <div class="row">
            <% for(var i=0; i<electives.length; i++) {%>
                <div class="col-md-3">
                    <a href="/quant/<%=electives[i].name%>" class="quant-card d-flex flex-column justify-content-center"><img src="/img/<%=electives[i].img%>"></a>
                </div>
            <% } %>
        </div>
    </div>
</div>